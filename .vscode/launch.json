        {
            "name": "Attach client",
            "type": "node",
            "request": "attach",
            "port": 5959,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}/ui",
            "remoteRoot": "/ui",
            "timeout": 10000
        }
    ]
}